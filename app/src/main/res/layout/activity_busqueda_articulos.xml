<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg_temi"
    tools:context=".BusquedaArticulos">

    <ImageButton
        android:id="@+id/btnBack1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="77dp"
        android:background="@mipmap/exit_out" />

    <ImageButton
        android:id="@+id/btnRefresco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="244dp"
        android:layout_marginBottom="317dp"
        android:background="@mipmap/refrescos_out">

    </ImageButton>

    <ImageButton
        android:id="@+id/btnDulceria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="243dp"
        android:layout_marginBottom="317dp"
        android:layout_toEndOf="@+id/btnVinosLicores"
        android:background="@mipmap/dulceria_out" />

    <ImageButton
        android:id="@+id/btnVinosLicores"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="65dp"
        android:layout_marginBottom="317dp"
        android:layout_toEndOf="@+id/btnRefresco"
        android:background="@mipmap/vinos_out" />

</RelativeLayout>